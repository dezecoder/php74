 1000000000 === pow(10,9)
 100000000  === pow(-10,8)
 1    === pow(-1,1443279822)
-1    === pow(-1,1443279821)
sqrt(2) ~== pow(2,1/2)
 0.25 === pow(-2.0,-2.0)
-0.5  === pow(-2.0,-1.0)
 1.0  === pow(-2.0, 0.0)
-2.0  === pow(-2.0, 1.0)
 4.0  === pow(-2.0, 2.0)
 1.0  === pow(-1.0,-2.0)
-1.0  === pow(-1.0,-1.0)
 1.0  === pow(-1.0, 0.0)
-1.0  === pow(-1.0, 1.0)
 1.0  === pow(-1.0, 2.0)
 TRUE === is_infinite(pow(0.0,-2.0))
 TRUE === is_infinite(pow(0.0,-1.0))
 1.0  === pow( 0.0, 0.0)
 0.0  === pow( 0.0, 1.0)
 0.0  === pow( 0.0, 2.0)
 1.0  === pow( 1.0,-2.0)
