--TEST--
UUID::getVariant ignores Msb1 and Msb2 of octet 8 if Msb0 is 0
--CREDITS--
Richard Fussenegger php@fleshgrinder.com
--FILE--
<?php

use PHP\Std\UUID;

var_dump(
	UUID::fromBinary("\x00\x00\x00\x00\x00\x00\x00\x00\x60\x00\x00\x00\x00\x00\x00\x00")->getVariant(),
	UUID::fromBinary("\x00\x00\x00\x00\x00\x00\x00\x00\x40\x00\x00\x00\x00\x00\x00\x00")->getVariant(),
	UUID::fromBinary("\x00\x00\x00\x00\x00\x00\x00\x00\x20\x00\x00\x00\x00\x00\x00\x00")->getVariant(),
	UUID::fromBinary("\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00\x00")->getVariant()
);

?>
